    "indicator": {
      "s1_school_closing": {
        "en": "ordinal scale - Record closings of schools and universities"
      },
      "s1_isgeneral": {
        "en": "geographic scope - closings of schools and universities"
      },
      "s2_workplace_closing": {
        "en": "ordinal scale - Record closings of workplaces"
      },
      "s2_isgeneral": {
        "en": "geographic scope - closings of workplaces"
      },
      "s3_cancel_public_events": {
        "en": "ordinal scale - Record cancelling public events"
      },
      "s3_isgeneral": {
        "en": "geographic scope - cancelling public events"
      },
      "s4_close_public_transport": {
        "en": "ordinal scale - Record closing of public transport"
      },
      "s4_isgeneral": {
        "en": "geographic scope - closing of public transport"
      },
      "s5_public_information_campaigns": {
        "en": "binary - Record presence of public info campaigns"
      },
      "s5_isgeneral": {
        "en": "geographic scope - presence of public info campaigns"
      },
      "s6_restrictions_on_internal_movement": {
        "en": "ordinal scale - Record restrictions on internal movement"
      },
      "s6_isgeneral": {
        "en": "geographic scope - restrictions on internal movement"
      },
      "s7_international_travel_controls": {
        "en": "ordinal scale - Record restrictions on international travel"
      },
      "s8_fiscal_measures": {
        "en": "USD value of fiscal stimuli, including spending or tax cuts - Economic stimulus policies adopted"
      },
      "s9_monetary_measures": {
        "en": "% value of interest rate - Monetary policy interventions"
      },
      "s10_emergency_investment_in_health_care": {
        "en": "USD Value of new short-term spending on health - Short-term spending on, e.g, hospitals, masks, etc"
      },
      "s11_investment_in_vaccines": {
        "en": "USD Value of investment - Announced public spending on vaccine development"
      },
      "s12_testing_framework": {
        "en": "ordinal scale - Who can get tested"
      },
      "s13_contact_tracing": {
        "en": "ordinal scale - Are governments doing contact tracing"
      },
